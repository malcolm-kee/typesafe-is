{
  "name": "typesafe-is",
  "version": "1.0.1",
  "description": "Check the type of a value with typescript typeguard",
  "main": "index.js",
  "keywords": [
    "typescript",
    "typecheck",
    "type guard"
  ],
  "scripts": {
    "test": "jest",
    "test:ci": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "prebuild": "rimraf dist",
    "copy": "copy package.json dist && copy README.md dist",
    "compile": "tsc --p tsconfig.json",
    "build": "npm run compile && npm run copy"
  },
  "author": "Malcolm Kee <malcolm.keeweesiong@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/malcolm-kee/typesafe-is"
  },
  "bugs": {
    "url": "https://github.com/malcolm-kee/typesafe-is/issues"
  },
  "homepage": "https://github.com/malcolm-kee/typesafe-is#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "copy": "^0.3.2",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "test/.*.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  }
}
