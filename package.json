{
  "name": "typesafe-is",
  "version": "1.1.3",
  "description": "Check the type of a value with typescript typeguard",
  "main": "dist/index.js",
  "module": "dist/typesafe-is.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "typecheck",
    "type guard"
  ],
  "scripts": {
    "start": "tsdx watch",
    "test": "tsdx test",
    "test:ci": "tsdx test --coverage --coverageReporters=text-lcov | coveralls",
    "build": "tsdx build",
    "docs": "typedoc --out docs --mode modules"
  },
  "author": "Malcolm Kee <malcolm.keeweesiong@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/malcolm-kee/typesafe-is"
  },
  "bugs": {
    "url": "https://github.com/malcolm-kee/typesafe-is/issues"
  },
  "homepage": "https://github.com/malcolm-kee/typesafe-is#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "coveralls": "^3.0.4",
    "husky": "^3.0.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  }
}
